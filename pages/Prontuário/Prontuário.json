            {
              "id": "Prontuário_calculaDias.calcula",
              "name": "calculaDias.calcula",
              "collectionId": "Prontuário_calculaDias",
              "clientSideExecution": true,
              "confirmBeforeExecute": false,
              "pluginType": "JS",
              "jsonPathKeys": [
                "function () {\n  const tratamentosMedicos = get_animal_tratamento_medico.data || [];\n  const periodosDeDias = [];\n  tratamentosMedicos.data.forEach(tratamento => {\n    const dataInicio = new Date(tratamento.tratamento_medico.data_inicio);\n    const dataFim = new Date(tratamento.tratamento_medico.data_fim);\n    const diferencaEmMilissegundos = dataFim - dataInicio;\n    const diferencaEmDias = Math.floor(diferencaEmMilissegundos / (1000 * 60 * 60 * 24));\n    periodosDeDias.push(diferencaEmDias);\n  });\n  return periodosDeDias;\n}"
              ],
              "timeoutInMillisecond": 10000
            },
            {
              "id": "Prontuário_mapeaDoenca.myFun1",
              "name": "mapeaDoenca.myFun1",
              "collectionId": "Prontuário_mapeaDoenca",
              "clientSideExecution": true,
              "confirmBeforeExecute": false,
              "pluginType": "JS",
              "jsonPathKeys": [
                "function () {\n  const tratamentosMedicosPorId = get_animal_tratamento_medico.data.data.reduce((acc, item) => {\n    acc[item.id] = item;\n    return acc;\n  }, {});\n  const nomesDasDoencas = [];\n  get_e_causado.data.data.forEach(item => {\n    const tratamentoMedico = tratamentosMedicosPorId[item.tratamento_medico.id];\n    if (tratamentoMedico) {\n      const nomeDaDoenca = item.doenca.nome;\n      nomesDasDoencas.push(nomeDaDoenca);\n    }\n  });\n  return nomesDasDoencas;\n}"
              ],
              "timeoutInMillisecond": 10000
            }
          ],
          [
            {
              "id": "Prontuário_calculaIdade.calcula",
              "name": "calculaIdade.calcula",
              "collectionId": "Prontuário_calculaIdade",
              "clientSideExecution": true,
              "confirmBeforeExecute": false,
              "pluginType": "JS",
              "jsonPathKeys": [
                "function () {\n  let tratamentosMedicos = get_animal_tratamento_medico.data || [];\n  tratamentosMedicos.data.forEach(tratamento => {\n    const dataInicio = new Date(tratamento.animal.Data_nascimento);\n    const dataAtual = new Date();\n    const diferencaEmAnos = dataAtual.getFullYear() - dataInicio.getFullYear();\n    if (dataInicio.getMonth() > dataAtual.getMonth() || dataInicio.getMonth() === dataAtual.getMonth() && dataInicio.getDate() > dataAtual.getDate()) {\n      tratamento.animal.Idade = diferencaEmAnos - 1;\n    }\n  });\n  return tratamentosMedicos;\n}"
              ],
              "timeoutInMillisecond": 10000
            }
